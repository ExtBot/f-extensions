{
    "name": "xengine/flarum-ext-signature",
    "description": "Signature support to your Flarum forum.",
    "authors": [
        {
            "name": "Extum",
            "email": "ardacebi1@gmail.com"
        }
    ],
    "type": "flarum-extension",
    "require": {
        "flarum/core": "^0.1.0-beta.7",
        "symfony/dom-crawler":"^3.1",
        "symfony/css-selector":"^3.1"
    },
    "autoload": {
        "psr-4": {
            "XEngine\\Signature\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Signature",
            "icon" : {
                "name" : "pencil-square",
                "backgroundColor" : "#000",
                "color": "#DC3522"
            }
        }
    }
}
