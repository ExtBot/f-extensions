{
  "name": "mprove/flarum-ext-auth0",
  "description": "Auth0 extension for Flarum",
  "type": "flarum-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Maxim Kvasha",
      "email": "kmi.maxim@gmail.com"
    }
  ],
  "require": {
    "flarum/core": "^0.1.0-beta.6",
    "auth0/auth0-php": "~5.0"

  },
  "extra": {
    "flarum-extension": {
      "title": "SSO Auth0",
      "icon": {
        "image": "icon.png",
        "backgroundColor": "#e4e4e4",
        "backgroundRepeat": "no-repeat",
        "backgroundPosition": "center"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Mprove\\Auth\\Auth0\\": "src/",
      "Auth0\\SDK\\": "../../auth0/auth0-php/src"
    }
  }
}